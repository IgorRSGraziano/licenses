<p style="color: green"><%= notice %></p>
<h1>Import licenses</h1>
<br>
<div>
  <%= form_with action_name: licenses_import_path do |form| %>
    <%= form.label :licenses, 'Separate licenses by ";"', class: 'block' %>
    <%= form.text_area :licenses, placeholder: 'license1;license2;license3', class: 'block my-7' %>
    <%= form.submit 'Save', class: 'button' %>
  <% end %>
  <%= link_to 'Back to licenses', licenses_path %>
</div>
